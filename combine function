function add(a,b) {
    let c = a+b;
    console.log(c);
}
     

function sub(x,y) {
    let z = x-y;
    console.log(z);
}

function combine(add,sub)
{
     let  c = add(5,5);
     let  z = sub(100,50);

}
combine(add,sub);

















































































































